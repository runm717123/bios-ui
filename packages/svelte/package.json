{
	"name": "@bios-ui/svelte",
	"type": "module",
	"version": "0.0.1",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./css": {
			"import": "./dist/svelte.css"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && tsc --project tsconfig.app.json",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
		"pub": "pnpm publish --access public --no-git-checks"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tsconfig/svelte": "^5.0.4",
		"svelte": "^5.28.1",
		"svelte-check": "^4.1.6",
		"typescript": "~5.8.3",
		"vite": "^6.3.5"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@bios-ui/core": "workspace:*"
	}
}